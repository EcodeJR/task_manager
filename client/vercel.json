{
  "version": 2,
  "builds": [
    { "source": "src/index.js", "use": "@vercel/static-build", "config": { "distDir": "build" } }
  ],
  "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
